{
  "manifest_version": 3,
  "name": "CSV Everything",
  "version": "1.0.0",
  "description": "Convert images of tables or charts into downloadable CSV files",
  "permissions": [
    "clipboardRead",
    "storage",
    "downloads"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "CSV Everything"
  },
  "options_page": "options.html",
  "host_permissions": [
    "https://openrouter.ai/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
